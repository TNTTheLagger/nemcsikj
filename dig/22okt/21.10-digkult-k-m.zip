                      function helyescim(emcim) {
                        if (emcim.length<8) {
                          return false;
                        }
                        else if (emcim.lastIndexOf("@") < 2) {
                          return false;
                        }
                        else if (emcim.lastIndexOf(".") < 5) {
                          return false;
                        }
                        else if (emcim.lastIndexOf(".") < emcim.lastIndexOf("@")) {
                          return false;
                        }
                        else {
                          return true;
                        }
                      }

